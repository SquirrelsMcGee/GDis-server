{
  "name": "gdis-server",
  "version": "1.0.0",
  "scripts": {
    "start:prod": "node dist/index.js",
    "build:prod": "tsup src/index.ts --minify",
    "clean:build": "rimraf dist/.built",
    "build:watch": "npm run clean:build && tsc-watch --noEmit --onSuccess \"npx touch-cli dist/.built\"",
    "run:watch": "wait-on dist/.built && tsx watch src/index.ts",
    "dev": "concurrently --kill-others --prefix \"[{name}]\" -n \"build, run \" --prefix-colors \"blue.bold,green.bold\" \"npm run build:watch\" \"npm run run:watch\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/lodash": "^4.17.13",
    "@types/md5": "^2.3.5",
    "concurrently": "^9.1.0",
    "md5": "^2.3.0",
    "rimraf": "^6.0.1",
    "touch-cli": "^0.0.1",
    "tsc-watch": "^6.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  }
}